{
    "mcpServers": {
        "serena": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server",
                "--context",
                "ide-assistant"
            ],
            "env": {}
        },
        "mcp-server-motherduck": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "mcp-server-motherduck",
                "--db-path",
                "md:",
                "--motherduck-token",
                "MOTHERDUCK_TOKEN_PLACEHOLDER"
            ],
            "env": {}
        },
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp"
        }
    }
}